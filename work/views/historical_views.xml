        <record id="view_hist_rea_list" model="ir.ui.view">
            <field name="name">hist rea list</field>
            <field name="model">work.hist_rea</field>
            <field name="arch" type="xml">
                <tree string="REA">
                    <field name="outsource_id"/>
                    <field name="date_expiration_rea"/>
                    <field name="rea_file_name"/>
                </tree>
            </field>
        </record>
        <record id="view_hist_rea_form" model="ir.ui.view">
            <field name="name">hist rea Form</field>
            <field name="model">work.hist_rea</field>
            <field name="arch" type="xml">
                <form string="REA">
                    <header>
                    </header>  
                    <sheet>
                        <group name = "arriba">   
                            <group name = "arriba_izquierda">
                                <field name="outsource_id"/>
                            </group>
                            <group name = "arriba_derecha">
                            </group>
                        </group>

                        <group name="centro">   
                            <group name = "centro_izquierda">
                                <field name="rea" filename="rea_file_name"/>
                                <field name="rea_file_name"  invisible="1"/>
                            </group>
                            <group name = "centro_derecha">
                                <field name="date_expiration_rea"/>
                            </group>
                        </group>

                    </sheet>
                </form>
            </field>
        </record>

